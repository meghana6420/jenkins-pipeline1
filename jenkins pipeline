pipeline {
    agent {
  label 'jenkins'
}
      stages {
        stage ('BUILD') {
          steps {
            echo ' this is BUILD stage'
          }
      }
    
stage ('TEST') {
          steps {
            echo ' this is TEST stage'
          }
      }
stage ('DEPLOY') {
          steps {
            echo ' this is DEPLOY stage'
          }
      }
    }
}
